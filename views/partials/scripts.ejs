<script >
	$('.LOGOUT').click(async (e) => {
		e.preventDefault()
		e.stopPropagation()
		
		await $.ajax({
			url: '/index',
			method: 'POST',
		}).done((data) => {
			$('#app').html(data)
			window.history.pushState(data, 'Title', '/index')
		})
	})
</script>



