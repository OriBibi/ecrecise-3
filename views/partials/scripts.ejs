<script >
	$('.nav-link').click(async (e) => {
		e.preventDefault()
		e.stopPropagation()
		let url = '/' + e.target.id.split('-')[1]
		await $.ajax({
			// url = '/' + nav-index
			url: url,
			method: 'POST'
		}).done((data) => {
			$('#app').html(data)
			window.history.pushState(data, 'Title', url)
		})
	})
</script>
